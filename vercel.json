{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "production",
    "CANVAS_EXTENSION_SERVICE_URL": "https://canvas-service-final-s6vo3k273a-uc.a.run.app"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "functions": {
    "app/api/images/extend-canvas/route.ts": {
      "maxDuration": 60,
      "memory": 1536
    },
    "app/api/images/extend-canvas-js/route.ts": {
      "maxDuration": 60,
      "memory": 1536
    },
    "app/api/images/extend-canvas-remote/route.ts": {
      "maxDuration": 60,
      "memory": 1536
    }
  }
}
